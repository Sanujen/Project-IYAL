{
  "openapi": "3.1.0",
  "info": { "title": "FastAPI", "version": "0.1.0" },
  "paths": {
    "/analyze/": {
      "post": {
        "summary": "Analyze Input",
        "description": "Analyzes the input text and returns tuple containing the normalized Tamil Unicode text and an array of dictionaries containing the classification results.\n\nArgs:\n    request (AnalyzeInputRequest): The input request containing the text to analyze.\n\nReturns:\n    dict: A dictionary containing the normalized Tamil Unicode text and the classification results.",
        "operationId": "analyze_input_analyze__post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/AnalyzeInputRequest" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/legacy2unicode/": {
      "post": {
        "summary": "Legacy2Unicode",
        "description": "Converts legacy Tamil text to Unicode.\n\nArgs:\n    request (Legacy2UnicodeInputRequest): The input request containing the legacy Tamil text to convert.\n\nReturns:\n    dict: A dictionary containing the converted Unicode text.",
        "operationId": "legacy2unicode_legacy2unicode__post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Legacy2UnicodeInputRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/get_encoding/": {
      "post": {
        "summary": "Get Encoding",
        "description": "Gets the encoding of the input text.\n\nArgs:\n    request (InputRequest): The input request containing the text to analyze.\n\nReturns:\n    dict: A dictionary containing the encoding of the input text.",
        "operationId": "get_encoding_get_encoding__post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/InputRequest" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/colloquial_to_standard/": {
      "post": {
        "summary": "Colloquial To Standard",
        "description": "Translates colloquial Tamil text to standard Tamil.\n\nArgs:\n    request (InputRequest): The input request containing the colloquial Tamil text to translate.\n\nReturns:\n    dict: A dictionary containing the translated standard Tamil text.",
        "operationId": "colloquial_to_standard_colloquial_to_standard__post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/InputRequest" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AnalyzeInputRequest": {
        "properties": {
          "input_text": { "type": "string", "title": "Input Text" },
          "encoding": { "type": "string", "title": "Encoding" },
          "need_translation": {
            "type": "boolean",
            "title": "Need Translation",
            "default": false
          },
          "colloquial_to_standard": {
            "type": "boolean",
            "title": "Colloquial To Standard",
            "default": false
          }
        },
        "type": "object",
        "required": ["input_text"],
        "title": "AnalyzeInputRequest"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "InputRequest": {
        "properties": {
          "input_text": { "type": "string", "title": "Input Text" }
        },
        "type": "object",
        "required": ["input_text"],
        "title": "InputRequest"
      },
      "Legacy2UnicodeInputRequest": {
        "properties": {
          "input_text": { "type": "string", "title": "Input Text" },
          "encoding": { "type": "string", "title": "Encoding" }
        },
        "additionalProperties": true,
        "type": "object",
        "required": ["input_text"],
        "title": "Legacy2UnicodeInputRequest"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
            "type": "array",
            "title": "Location"
          },
          "msg": { "type": "string", "title": "Message" },
          "type": { "type": "string", "title": "Error Type" }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    }
  }
}
